<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="243.0" prefWidth="539.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rs.emulate.editor.core.project.actions.OpenProjectDialog">
    <children>
          <ScrollPane fx:id="formPane" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS" />
          <HBox alignment="CENTER_RIGHT" prefHeight="49.0" prefWidth="539.0">
              <children>
                  <Button fx:id="openButton" mnemonicParsing="false" text="Open">
                      <HBox.margin>
                          <Insets left="10.0" right="5.0" />
                      </HBox.margin>
                  </Button>
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel">
                      <HBox.margin>
                          <Insets left="5.0" right="10.0" />
                      </HBox.margin>
                  </Button>
              </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
          </HBox>
    </children>
</VBox>
