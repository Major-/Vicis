<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import org.controlsfx.control.StatusBar?>
<?import rs.emulate.editor.javafx.controls.ResizableBorderPane?>
<BorderPane fx:controller="rs.emulate.editor.core.workbench.Workbench" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="/styles.css" styleClass="workbench">
    <top>
        <MenuBar fx:id="menuBar"/>
    </top>
    <center>
        <ResizableBorderPane styleClass="dock-pane">
            <center>
                <TabPane fx:id="editorDock"  />
            </center>
            <left>
                <TabPane fx:id="leftDock" />
            </left>
            <right>
                <TabPane fx:id="rightDock" />
            </right>
            <bottom>
                <TabPane fx:id="bottomDock"  />
            </bottom>
        </ResizableBorderPane>
    </center>
    <bottom>
        <StatusBar fx:id="statusBar"/>
    </bottom>
</BorderPane>

